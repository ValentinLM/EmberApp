<div class="container-login">
    <h1>Login</h1>
    <form {{on "submit" this.login}}  >
        
        <label for="username">Username</label>
        <input type="text" name="username" {{on "change" (fn this.update "usarname")}}>
        
        
        <label>Password</label>
        <input type="password" {{on "change" (fn this.update "password")}}>
        
        <input type="submit" value="Login">
    </form>
</div>

{{!-- {{#if error}}
    <p><strong>{{error}}</strong></p>
{{/if}} --}}

<Footer::EndPage />
